    "eslint-config-hckrs": "^0.0.3",
    "np": "^7.6.3",
    "prettier": "^2.8.1",
    "tsx": "^3.12.1",
    "type-fest": "^3.5.1",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "prettier": {
    "singleQuote": true
